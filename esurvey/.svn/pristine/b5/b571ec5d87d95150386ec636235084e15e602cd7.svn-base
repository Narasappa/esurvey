<div class="page-header">
  <%= link_to new_survey_result_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Survey result
  <% end %>
  <h1>Listing survey_results</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Interview</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @survey_results) do |survey_result| %>
            <td><%= survey_result.interview %></td>
            <td><%= link_to 'Show', survey_result %></td>
        <td><%= link_to 'Edit', edit_survey_result_path(survey_result) %></td>
        <td><%= link_to 'Destroy', survey_result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
